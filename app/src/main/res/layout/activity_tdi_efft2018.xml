<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="match_parent"
    tools:context=".TDI_EFFT_2018">
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:src="@drawable/guauge"
        android:id="@+id/guaugeEFF"/>
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:src="@drawable/aguille"
        android:id="@+id/aguilleEFF"
        android:layout_marginTop="-17dp"
        android:layout_marginRight="50dp"/>
    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Entrez vitesse : "
        android:inputType="number"
        android:id="@+id/txtVitesseEFF2018"/>
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Calculer"
        android:id="@+id/btnCEFF2018"/>
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Effacer"
        android:id="@+id/btnSEFF2018"/>

</LinearLayout>